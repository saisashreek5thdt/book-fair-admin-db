generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // Or "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
  role     String   @default("USER") // Admin, User, etc.
  createdAt DateTime @default(now())
}
